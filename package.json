{
  "name": "sikb-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "cypress:ci": "run-p --race start:ci cypress:run",
    "start:ci": "http-server ./dist -a localhost -p 8080 -c-1",
    "cypress:run": "cypress run --record --key 7a9db2c4-3116-4498-b2f6-d5588efc6080",
    "lint": "vue-cli-service lint",
    "lint fix": "vue-cli-service lint fix",
    "test:unit": "jest --detectOpenHandles --no-cache --config jest-coverage.config.js",
    "coveralls": "jest --runInBand --no-cache --config jest-coverage.config.js --coverage && cat ./coverage/lcov.info | coveralls",
    "swagger": "openapi-generator generate -i http://sikb-api.eu-west-3.elasticbeanstalk.com/swagger-ui/swagger.yaml -t templates -c swagger-config.json -g typescript-axios -o src/generated --skip-validate-spec"
  },
