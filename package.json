    "@types/jest": "23.3.10",
    "@types/jest-environment-puppeteer": "^4.0.0",
    "@types/puppeteer": "^1.12.4",
    "@types/webpack": "^4.4.32",
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-typescript": "^3.7.0",
